<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="spl.tools.AstVisualizer"
            prefHeight="600.0" prefWidth="800.0">
    <TreeTableView fx:id="treeView">
        <columns>
            <TreeTableColumn fx:id="typeCol" text="Type" prefWidth="200.0"/>
            <TreeTableColumn fx:id="valueCol" text="Value" prefWidth="200.0"/>
            <TreeTableColumn fx:id="msgCol" text="Message" prefWidth="200.0"/>
            <TreeTableColumn fx:id="lineCol" text="Line" prefWidth="50.0"/>
            <TreeTableColumn fx:id="fileCol" text="File" prefWidth="150.0"/>
        </columns>
    </TreeTableView>


</VBox>
